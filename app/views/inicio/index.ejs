<div class="page-content">

	



	<section class="no-padding-bottom">
		<div class="container-fluid">
			<div class="card">
				<div class="row">
					<div class="col-sm-12">
						<div class="text-center bg-background-1 header-actions">
							IBOVESPA
							
						</div>
						
					</div>


				</div>

				<div class="row">
					<div class="col-sm-12">



						<div class="slider-container">
							<div class="slide-item">
								<img src="/public/images/pdf/00.png">
								<img src="/public/images/pdf/01.png">
							</div>
						</div>
						
					</div>

				</div>

			</div>
		</div>
	</section>

	<section>
		<div class="container-fluid">
			<div class="row">
				<div class="col-sm-12 col-md-6">
					<div class="card">
						<div class="padding-20">

							<% if(data[usuario.id +'_usuario_corretora'].length == 0){ %>

							<%-include('conexao_corretora')%>

							<%}else if(data[usuario.id +'_usuario_corretora'][0].testado == false && data[usuario.id +'_usuario_corretora'][0].validado == false){ %>

							<div class="alert alert-warning">Sua conta ainda está sendo testada por-favor esperar entre 1-3 dias</div>

							<%}else if(data[usuario.id +'_usuario_corretora'][0].testado == true && data[usuario.id +'_usuario_corretora'][0].validado == true){%>
							<div class="alert alert-success">Sua conta foi verificada com sucesso!</div>

							<%}else{%>
							<div class="alert alert-danger">Algum dado está incorreto, por-favor verificar!</div>
							
							<%-include('conexao_corretora')%>
							

							<%}%>


						</div>

					</div>
					
				</div>
				<div class="col-sm-12 col-md-6">
					<div class="card">
						
						GRÁFICO MQL5
					</div>
					
				</div>
			</div>
		</div>
	</section>






