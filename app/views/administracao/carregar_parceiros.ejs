<% if(usuario.nivel == 1){%>

<label for="nivel" class="col-sm-2 col-form-label">Parceiro</label>
<div class="col-sm-10">
	<select class="form-control" aria-label="Default select example" name="id_parceiro">
		<option selected>Não possui</option>
		<% if (typeof data[usuario.id +'_parceiros'] != 'undefined' && data[usuario.id +'_parceiros'].length > 0) {
			for (var i = 0 ; i < data[usuario.id +'_parceiros'].length; i++) {
				%>
				<option value="<%=data[usuario.id +'_parceiros'][i]._id  %>"><%=data[usuario.id +'_parceiros'][i].nome  %></option>
				<%}
			}
			%>


		</select>
	</div>




	<%}else{%>
	<div class="page-content">
		<div class="container-fluid">
			<div class="alert error-block margin-t-10">
				Você não pode acessar essa página!
			</div>
		</div>
	</div>


	<%}%>