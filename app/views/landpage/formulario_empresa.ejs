<script type="text/javascript">

	<% if( typeof message.regras_algoritmo != 'undefined'){ %>
	var media_ano = <%=message.regras_algoritmo[0].media_ano%>;
	var volatilidade = <%= message.regras_algoritmo[0].volatilidade %>;



	console.log('media_ano: ' + media_ano);
	console.log('volatilidade: ' + volatilidade);

	$(document).on('input', '.range-empresa', function(e) {
		e.preventDefault();

		console.log('estou alterando o range');
		console.log($(this).val());

		var capital = parseFloat($(this).val());

		console.log('capital_inicial: ' + capital);


		var capital_form = capital.toLocaleString('pt-br', {minimumFractionDigits: 2});
		console.log('capital_form: '+ capital_form);

		$('#capital_inicial').text(capital_form);

		console.log('possui 30 dias g');
		console.log($('.possui_30_dias_g').val());

		if($('.possui_30_dias_g').val() == '1'){
			<%-include('function_barra_gratis_empresa')%>;
		}else{
			<%-include('function_barra_pago_empresa')%>;
		}



	});

	$(document).on('input', '.range-fisica', function(e) {
		e.preventDefault();

		console.log('estou alterando o range');
		console.log($(this).val());

		var capital = parseFloat($(this).val());
		var capital_form = capital.toLocaleString('pt-br', {minimumFractionDigits: 2});
		console.log('capital_form: '+ capital_form);

		$('#capital_inicial').text(capital_form);

		console.log('possui 30 dias g');
		console.log($('.possui_30_dias_g').val());

		if($('.possui_30_dias_g').val() == '1'){
			<%-include('function_barra_gratis_empresa_menor2kk')%>;
		}else{
			<%-include('function_barra_pago_fisico')%>;
		}



	});









	function SubmitLandpageConhecer(post, link,element) {
		var capital_aportado;

		$.ajax({
			method: 'POST',
			async: true,
			data: post,
			url: link,
			beforeSend: function(request) {
			},
			success: function(data) {

				console.log('retorno da data');
				console.log(data);
				console.log('xxxxxxxxxxxxxxxxxxxxxxxxxxx');
				console.log('yay2');
				console.log('data.pretencao_investimento');
				console.log(data.pretencao_investimento);


				var capital = data.pretencao_investimento;


				if(capital>0){
					$(element).removeClass('none');
					$('html,body').animate({
						scrollTop: $(element).offset().top},
						'slow');



					var capital_form = capital.toLocaleString('pt-br', {minimumFractionDigits: 2});
					$('.range-empresa').val(capital);


					console.log('capital_form: '+ capital_form);

					$('#capital_inicial').text(capital_form);





					if(data.possui_30dias == 'gratis' || data.possui_30dias == 'gratis_80'){
						console.log('cai no gratis');

						$('.possui_30_dias_g').val('1');


						$('.voce_possui_30_dias_gratuitos').removeClass('none');

						if(capital <= 1000000){
							$('.form-control-range').removeClass('range-empresa');
							$('.form-control-range').addClass('range-fisica');


							$('.form-control-range').prop({
								'min': 80000,
								'max': 1500000,
								'step':10000
							});

							$('.form-control-range').val(capital);
							<%-include('function_barra_gratis_empresa_menor2kk')%>;


						}else{
							$('.form-control-range').removeClass('range-fisica');
							$('.form-control-range').addClass('range-empresa');

							$('.form-control-range').prop({
								'min': 2000000,
								'max': 500000000,
								'step':1000000
							});

							<%-include('function_barra_gratis_empresa')%>;
						}




						
					}else{
						console.log('cai no pago');




						$('.possui_30_dias_g').val('2');
						$('.voce_possui_30_dias_gratuitos').addClass('none');

						console.log('capital: ' + capital);

						if(capital <= 1000000){

							console.log('cai aqui no capital menor que 1kk')


							$('.form-control-range').removeClass('range-empresa');
							$('.form-control-range').addClass('range-fisica');


							$('.form-control-range').prop({
								'min': 80000,
								'max': 1500000,
								'step':10000
							});

							$('.form-control-range').val(capital);


							<%-include('function_barra_pago_fisico')%>;
						}else{


							$('.form-control-range').removeClass('range-fisica');
							$('.form-control-range').addClass('range-empresa');


							$('.form-control-range').prop({
								'min': 2000000,
								'max': 500000000,
								'step':1000000
							});



							<%-include('function_barra_pago_empresa')%>;
						}
					}




					$('.mensagem_agredecimento_formulario').addClass('none');


				}else{
					$(element).addClass('none');
					$('.mensagem_agredecimento_formulario').removeClass('none');
				}







			},
			error: function(xhr) {
			},
			complete: function() {



			}
		});
	}



	<%}%>








</script>